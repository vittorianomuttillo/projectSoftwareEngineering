<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.entity.ETimeRecord" table="ETimeRecord" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="timeInizio" column="TimeInizio" type="date" not-null="true" lazy="false"/>
		<property name="timeFine" column="TimeFine" type="date" not-null="true" lazy="false"/>
		<property name="secondi" column="Secondi" type="long" length="50" not-null="false" lazy="false"/>
		<property name="descrizione" column="Descrizione" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
