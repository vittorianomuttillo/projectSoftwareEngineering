<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.entity.EStipendio" table="EStipendio" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="ammontare" column="Ammontare" type="double" not-null="true" lazy="false"/>
		<property name="unita" column="Unita" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
