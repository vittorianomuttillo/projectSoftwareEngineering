<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.entity.EManager" table="EManager" lazy="false">
		<id name="idManager" column="IdManager" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="codiceFiscale" column="CodiceFiscale" type="string" length="255" not-null="false" lazy="false"/>
		<property name="nomeManager" column="NomeManager" type="string" length="255" not-null="false" lazy="false"/>
		<property name="cognomeManager" column="CognomeManager" type="string" length="255" not-null="false" lazy="false"/>
		<property name="_saved" column="_saved" type="boolean" length="1" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
